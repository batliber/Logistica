<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="uy.com.amensg.logistica.persistenceUnit" transaction-type="JTA">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<jta-data-source>java:PostgreSQLLogistica</jta-data-source>
        
		<class>uy.com.amensg.logistica.entities.ACMInterfaceContrato</class>
		<class>uy.com.amensg.logistica.entities.ACMInterfaceListaNegra</class>
		<class>uy.com.amensg.logistica.entities.ACMInterfaceMid</class>
		<class>uy.com.amensg.logistica.entities.ACMInterfacePrepago</class>
		<class>uy.com.amensg.logistica.entities.ACMInterfaceProceso</class>
		<class>uy.com.amensg.logistica.entities.SeguridadAuditoria</class>
		<class>uy.com.amensg.logistica.entities.SeguridadTipoEvento</class>
		<class>uy.com.amensg.logistica.entities.Usuario</class>
		<class>uy.com.amensg.logistica.entities.StockMovimiento</class>
		<class>uy.com.amensg.logistica.entities.Producto</class>
		
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect"/>
			<!--
			<property name="hibernate.show_sql" value="true"/> 
			<property name="hibernate.hbm2ddl.auto" value="create" />
			 -->
		</properties>
	</persistence-unit>
</persistence>